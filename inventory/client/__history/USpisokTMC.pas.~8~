uses
  USelect;

{$R *.dfm}

procedure TFMSpisokTMC.FormShow(Sender: TObject);
begin
  if pomechenie<>'' then
  else
    if pomechenie='balok1' then
      Label1.Caption:='balok1'
    else
    if pomechenie='balok2' then
      Label1.Caption:='balok2'
end;

end.
